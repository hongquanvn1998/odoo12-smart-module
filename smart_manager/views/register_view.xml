                        <!-- <button name="" type="object" string="Đã xác minh" groups="" class="oe_highlight" attrs="{'invisible':[('state','!=','expired')]}"/> -->
                        <button name="" type="object" string="Kích hoạt" groups="" class="oe_highlight" attrs="{'invisible':[('state','!=','expired')]}" />
                        <field name="state" readonly="1" widget="statusbar" />

                    </header>
                    <sheet>
                        <group class="oe_title o_horizontal_separator">
                            <field name="code" class="bg-primary p-3 m-0 border" nolabel="1" readonly="1"  /> 
                        </group>
                        <group string="Partner Login">
                            <field name="use_type" readonly="1"/>
                            <field name="shop_domain" required="1" />
                            <field name="username" required="1" />
