<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="smart_manager_register_view_kanban" model="ir.ui.view">
            <field name="name">smart.manager.register.view.kanban</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <kanban create="false" class="o_kanban_dashboard o_pos_kanban">
                    <field name="shop_domain" />
                    <field name="name" />
                    <field name="activated_date" />
                    <field name="expired_date"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div>
                                <div class="o_kanban_card_header">
                                    <div class="o_kanban_card_header_title mb16">
                                        <div class="o_primary">
                                        <field name="shop_domain"/>
                                        <field name="name"/>
                                        </div>
                                        
                                    </div>
                                    
                                    <div class="o_kanban_manage_button_section">
                                        <a class="o_kanban_manage_toggle_button" href="#"><i class="fa fa-ellipsis-v" role="img" aria-label="Manage" title="Manage"/></a>
                                    </div>
                                </div>
                                <div class="container o_kanban_card_content">
                                    <div class="row">
                                        <div class="col-6 o_kanban_primary_left">
                                            <button class="btn btn-primary" name="get_traffic" type="object">View traffic</button>
                                        </div>
                                        <div class="col-6 o_kanban_primary_right">
                                            <div  class="row" t-if="record.activated_date.value">
                                                <div class="col-6">
                                                    <span>Activated date:</span>
                                                </div>
                                                <div class="col-6">
                                                    <span><t t-esc="record.activated_date.value"/></span>
                                                </div>
                                            
                                            </div>
                                            <div  class="row" t-if="record.expired_date.value">
                                                <div class="col-6">
                                                    <span>Expired date:</span>
                                                </div>
                                                <div class="col-6">
                                                    <span><t t-esc="record.expired_date.value"/></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <!-- List View  -->
        <record model="ir.ui.view" id="smart_manager_register_view_tree">
            <field name="name">Registered Shop List</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code" />
                    <field name="name" />
                    <field name="register_date" />
                    <field name="register_quantity" />
                    <field name="register_type" />
                    <field name="use_type" />
                    <field name="ward" />
                    <field name="district" />
                    <field name="province" />
                    <field name="state" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="smart_manager_verifying_view_tree">
            <field name="name">Verifying Shop List</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <tree  create="false">
                    <field name="code" />
                    <field name="name" />
                    <field name="register_date" />
                    <field name="register_quantity" />
                    <field name="register_type" />
                    <field name="use_type" />
                    <field name="ward" />
                    <field name="district" />
                    <field name="province" />
                    <field name="state" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="smart_manager_activated_view_tree">
            <field name="name">Activated Shop List</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <tree  create="false">
                    <field name="code" />
                    <field name="name" />
                    <field name="use_type" />
                    <field name="register_date" />
                    <field name="activated_date" />
                    <field name="expired_date" />
                    <field name="district" />
                    <field name="province" />
                    <field name="state" />
                </tree>
            </field>
        </record>
 
        <record model="ir.ui.view" id="smart_manager_expired_view_tree">
            <field name="name">Expired Shop List</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <tree  create="false">
                    <field name="code" />
                    <field name="name" />
                    <field name="use_type" />
                    <field name="activated_date" />
                    <field name="register_type" />
                    <field name="register_quantity" />
                    <field name="expired_date" />
                    <field name="district" />
                    <field name="province" />
                    <field name="state" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="smart_manager_cancelled_view_tree">
            <field name="name">Cancelled Shop List</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <tree  create="false">
                    <field name="code" />
                    <field name="name" />
                    <field name="use_type" />
                    <field name="note" />
                    <field name="state" />
                </tree>
            </field>
        </record>

        <!-- Form View  -->

        <!-- register form view -->
        <record id="smart_manager_register_view_form" model="ir.ui.view">
            <field name="name">smart_manager.register.view.form</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <form string="Register form">
                    <header>
                        <button name="action_register_register" type="object" string="Register Now" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('draft'))]}" />
                        <button name="action_register_verify" type="object" string="Verify Now" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('registered'))]}" />
                        <button name="action_register_to_draft" type="object" string="Redraft" groups="" attrs="{'invisible':[('state','not in',('registered'))]}" />
                        <button name="action_register_cancel" type="object" string="Cancel" groups="" attrs="{'invisible':[('state','not in',('registered','draft'))]}" />
                        <field name="state" readonly="1" widget="statusbar" />

                    </header>
                    <sheet>
                        <group class="oe_title o_horizontal_separator " attrs="{'invisible':[('id','=',False)]}">
                            <field name="code" class="bg-primary p-3 m-0 border" nolabel="1" readonly="1"  /> 
                        </group>
                        <group>
                            <group string="Customer Information">
                                <field name="partner_name" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="mobile" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="email" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                            

                            </group>
                            <group string="Account Information">
                                <field name="use_type" required="1" attrs="{'readonly':[('state','not in',('draft'))]}"/>
                                <field name="shop_domain" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="username" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="password" required="1" password="True" attrs="{'readonly':[('state','not in',('draft'))]}" />
                            </group>
                        </group>

                        <group string="Manager Information">
                            <group>
                                <field name="verify_type" required="1" />
                            </group>
                            <group>
                                <field name="register_date" />
                            </group>
                            <field name="note" />
                        </group>

                        <notebook colspan="4">
                            <page string="Business Information">
                                <group>
                                    <group string="Business">
                                        <field name="name" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <field name="business_tax_code" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <field name="business_type" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <field name="business_size" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <field name="business_category" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                    </group>

                                    <group string="Contact">
                                        <field name="business_tax_address" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <field name="province" attrs="{'readonly':[('state','not in',('draft'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"  />
                                        <field name="district" attrs="{'readonly':[('state','not in',('draft'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                        <field name="ward" attrs="{'readonly':[('state','not in',('draft'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                    </group>
                                </group>
                            </page>

                            <page string="Installed Apps">
                                <group string="Period">
                                    <group>
                                        <field name="register_type" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                    </group>
                                    <group>
                                        <field name="register_quantity" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                    </group>
                                </group>
                                <group string="Apps">
                                    <field name="install_apps" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                        <!-- <tree>
                                            <field name="default_code" />
                                            <field name="name" />
                                            <field name="uom_id" />
                                            <field name="list_price" widget="monetary" />
                                        </tree>
                                    </field> -->
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">

                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user" />
                        <field name="activity_ids" widget="mail_activity" />

                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record>

        <!-- verifying form view -->
        <record id="smart_manager_verifying_view_form" model="ir.ui.view">
            <field name="name">smart_manager.register.view.form</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <form string="Verifying form" create="false" attrs="{'edit':[('state','not in',('verified','initialing'))]}">
                    <header>
                        <button name="action_register_confirm" type="object" string="Confirm Now" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('verifying'))]}" />
                        <button name="action_register_init_db" type="object" string="Create Database" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('verified'))]}" />
                        <button name="action_register_re_verify" type="object" string="Re-Verify" groups=""  attrs="{'invisible':[('state','not in',('verified'))]}" /> 
                        <field name="state" readonly="1" widget="statusbar" /> 
                    </header>
                    <sheet>
                       <group class="oe_title o_horizontal_separator ">
                            <field name="code" class="bg-primary p-3 m-0 border" nolabel="1" readonly="1"  /> 
                        </group>
                        <group>
                            <group string="Customer Information">
                                <field name="partner_name" required="1" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                <field name="mobile" required="1" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                <field name="email" required="1" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                            </group>
                            <group string="Account Information">
                                <field name="use_type" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="shop_domain" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="username" required="1" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="password" required="1" password="True" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="partner" string="Customer Contact" attrs="{'invisible':[('state','not in',('verified'))]}" />
                            </group>
                        </group>

                        <group string="Manager Information" col="6" colspan="2">
                            
                     
                                <field name="register_date" readonly="1" />
 
                                <field name="verify_type" required="1"  attrs="{'readonly':[('state','not in',('verifying'))]}"/> 
                       
                                <field name="employee"   attrs="{'invisible':[('state','not in',('verified','verifying'))], 'readonly':[('state','not in',('verifying'))]}"  />
                            
                        </group>

                        <group>  
                             <field name="note" />
                        </group>

                        <notebook colspan="4">
                            <page string="Business Information">
                                <group>
                                    <group string="Business">
                                        <field name="name" required="1" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <field name="business_tax_code" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <field name="business_type" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <field name="business_size" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <field name="business_category" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                            

                                    </group>

                                    <group string="Contact">
                                        <field name="business_tax_address" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <field name="province" attrs="{'readonly':[('state','not in',('verifying'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                        <field name="district" attrs="{'readonly':[('state','not in',('verifying'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                        <field name="ward" attrs="{'readonly':[('state','not in',('verifying'))]}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                    </group>
                                </group>
                            </page>

                            <page string="Installed Apps">
                                <group string="Period">
                                    <group>
                                        <field name="register_type" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                    </group>
                                    <group>
                                        <field name="register_quantity" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                    </group>
                                </group>
                                <group string="Apps">
                                    <field name="install_apps" attrs="{'readonly':[('state','not in',('verifying'))]}" />
                                        <!-- <tree>
                                            <field name="default_code" />
                                            <field name="name" />
                                            <field name="uom_id" />
                                            <field name="list_price" widget="monetary" />
                                        </tree>
                                    </field> -->
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">

                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user" />
                        <field name="activity_ids" widget="mail_activity" />

                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record> 

        <!-- activated form view -->
        <record id="smart_manager_activated_view_form" model="ir.ui.view">
            <field name="name">smart_manager.activated.view.form</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <form string="Activated form" create="false" edit="false">
                    <header>
                        <button name="action_register_cancel" type="object" string="Cancel" />
                        <field name="state" readonly="1" widget="statusbar" />
                    </header>
                    <sheet>
                        <group class="oe_title o_horizontal_separator">
                            <field name="code" class="bg-primary p-3 m-0 border" nolabel="1" readonly="1"  /> 
                        </group>
                        <group>
                            <group string="Customer Information">
                                <field name="partner_name" required="1" readonly="1" />
                                <field name="mobile" required="1" readonly="1" />
                                <field name="email" required="1" readonly="1" />
                            

                            </group>
                            <group string="Account Information">
                                <field name="use_type" readonly="1"/>
                                <field name="shop_domain" readonly="1" />
                                <field name="username" readonly="1" />
                                <field name="password" readonly="1" />
                            </group>
                        </group>

                        <group string="Manager Information">
                            <group>
                                <field name="verify_type" readonly="1" />
                            </group>
                            <group>
                                <field name="register_date" readonly="1" />
                                <field name="expired_date" readonly="1" />
                            </group>
                            <field name="note" readonly="1" />
                        </group>

                        <notebook colspan="4">
                            <page string="Business Information">
                                <group>
                                    <group string="Business">
                                        <field name="name" required="1" readonly="1" />
                                        <field name="business_tax_code" readonly="1" />
                                        <field name="business_type" readonly="1" />
                                        <field name="business_size" readonly="1" />
                                        <field name="business_category" readonly="1" />
                                    </group>

                                    <group string="Contact">
                                        <field name="business_tax_address" readonly="1" />
                                        <field name="province" readonly="1" />
                                        <field name="district" readonly="1" />
                                        <field name="ward" readonly="1" />
                                    </group>
                                </group>
                            </page>

                            <page string="Installed Apps">
                                <group string="Period">
                                    <group>
                                        <field name="register_type" readonly="1" />
                                    </group>
                                    <group>
                                        <field name="register_quantity" readonly="1" />
                                    </group>
                                </group>
                                <group string="Apps">
                                    <field name="install_apps" readonly="1" />
                                        <!-- <tree>
                                            <field name="default_code" />
                                            <field name="name" />
                                            <field name="uom_id" />
                                            <field name="list_price" widget="monetary" />
                                        </tree>
                                    </field> -->
                                </group>
                            </page>

                            <page string="Billings">
                                <group>
                                    <group>
                                        <field name="partner" readonly="1" />
                                        <field name="employee" readonly="1" />
                                        <field name="order" readonly="1" />
                                    </group>

                                    <group>
                                        <field name="activated_date" readonly="1" />
                                        <field name="expired_date" readonly="1" />
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">

                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user" />
                        <field name="activity_ids" widget="mail_activity" />

                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record>


        <!-- expired form view -->
        <record id="smart_manager_expired_view_form" model="ir.ui.view">
            <field name="name">smart_manager.expired.view.form</field>
            <field name="model">smart_manager.register</field>
            <field name="arch" type="xml">
                <form string="Register form" create="false">
                    <header>
                        <button name="action_register_verifi" type="object" string="Xác minh" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('registered','draft'))]}" />
                        <button name="action_register_confirm" type="object" string="Đã xác minh" groups="" class="oe_highlight" attrs="{'invisible':[('state','not in',('verifying','expired'))]}" />
                        <button name="action_register_init_db" type="object" string="Khởi tạo database" groups="" class="oe_highlight" attrs="{'invisible':[('state','!=','verified')]}" />
                        <button name="action_register_cancel" type="object" string="Hủy" groups="" attrs="{'invisible':[('state','=','draft')]}" />
                        <!-- <button name="" type="object" string="Đã xác minh" groups="" class="oe_highlight" attrs="{'invisible':[('state','!=','expired')]}"/> -->
                        <button name="" type="object" string="Kích hoạt" groups="" class="oe_highlight" attrs="{'invisible':[('state','!=','expired')]}" />
                        <field name="state" readonly="1" widget="statusbar" />

                    </header>
                   
                    <sheet> 
                    <group class="oe_title o_horizontal_separator">
                        <field name="code" class="bg-primary p-3 m-0 border" nolabel="1" readonly="1"  /> 
                    </group> 
                        <group string="Partner Login">
                            <field name="use_type" readonly="1"/>
                            <field name="shop_domain" required="1" />
                            <field name="username" required="1" />
                            <field name="password" required="1" />
                            <field name="verify_type" /> 

                        </group> 

                        <notebook colspan="4">
                            <page string="Partner Infomation" attrs="{'invisible':[('state','in',('registered','draft'))]}">
                                <group>
                                    <field name="partner">
                                        <field name="name" />
                                        <field name="title" />
                                        <field name="mobile" />
                                        <field name="ward" readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                        <field name="district" readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}" />
                                        <field name="province"  readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
                                    </field>
                                </group>
                            </page>

                            <page string="Register Infomation" attrs="{'invisible':[('state','not in',('registered','draft'))]}">
                                <group>
                                    <group>
                                        <field name="partner_name" />
                                        <field name="business_type" widget="radio" options="{'horizontal': true}" autocomplete="False" />
                                        <field name="business_tax_code" />
                                        <field name="business_category" autocomplete="False" />
                                        <field name="business_size" autocomplete="False" />
                                    </group>
                                    <group>
                                        <field name="mobile" />
                                        <field name="email" />
                                        <field name="ward" readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
                                        <field name="district" readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
                                        <field name="province"  readonly="1" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
                                    </group>
                                </group>
                            </page>

                            <page string="App &amp; Note">
                                <group>
                                    <field name="install_apps" />
                                        <!-- <tree>
                                            <field name="name" />
                                        </tree>
                                    </field> -->
                                    <field name="note" />
                                </group>
                            </page>
                        </notebook>
                    </sheet>

                    <div class="oe_chatter">

                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user" />
                        <field name="activity_ids" widget="mail_activity" />

                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record>


        <!-- cancelled form view -->
        <record id="smart_manager_cancelled_view_form" model="ir.ui.view">
            <field name="name">smart_manager.cancelled.view.form</field>
            <field name="model">smart_manager.register</field>
