                                        <field name="title" />
                                        <field name="mobile" />
                                        <field name="ward" />
                                        <field name="district" />
                                        <field name="province" />
                                    </field>
                                </group>
                            </page>

                            <page string="Register Infomation" attrs="{'invisible':[('state','not in',('registered','draft'))]}">
                                <group>
                                    <group>
                                        <field name="partner_name" />
